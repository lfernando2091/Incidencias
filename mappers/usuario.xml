<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="usuario">
    <insert id="agregarUsuario">
        INSERT INTO usuario
        (
        usuario,
        email,
        token
        )
        VALUES
        ('${usuario}', '${email}', '${token}')
    </insert>
</mapper>