<div class="content-padder content-background">
            <div class="uk-section-small uk-section-default header">
                <div class="uk-container uk-container-large">
                    <h1><span class="ion-speedometer"></span> Periodo</h1>
                    <p>
                        Bienvenido, <%= user %>
                    </p>
                    <ul class="uk-breadcrumb">
                        <li><a href="/welcome">Home</a></li>
                        <li><span href="">Periodo</span></li>
                    </ul>
                </div>
            </div>
        <div class="content-background">
            <div class="uk-section-large">
                <div class="uk-container uk-container-large">
                    <div uk-grid class="uk-child-width-1-1@s uk-child-width-2-3@l">
                        <div class="uk-width-1-1@s uk-width-1-2@l uk-width-1-1@xl"></div>
                        <div class="uk-width-1-1@s uk-width-3-2@l uk-width-1-1@xl">
                            <div class="uk-card uk-card-default">
                                <div class="uk-card-body">
                                    <center>
                                        <h2>Registros </h2><br />
                                    </center>
                                    <form method="POST">
                                        <fieldset class="uk-fieldset">

                                            <div class="uk-margin">
                                                <div class="uk-position-relative">
                                                    <div class="uk-overflow-container">
                                                    <table class="uk-table uk-table-hover uk-table-striped data-table display" style="width:100%">
                                                        <thead>
                                                            <tr>
                                                                <th>Id</th>
                                                                <th>Nombre</th>
                                                                <th>Clave</th>
                                                                <th>Estatus</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <% results.forEach(function(item){ %>
                                                                <tr>
                                                                    <td><%= item.Id %></td>
                                                                    <td><%= item.Nombre %></td>
                                                                    <td><%= item.Clave %></td>
                                                                    <td><%= item.Estatus %></td>
                                                                </tr>
                                                            <% })%>
                                                        </tbody>
                                                    </table>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="uk-margin">
                                                <div class="uk-position-relative">
                                                    <span class="uk-form-icon ion-locked"></span>
                                                    <input name="password" class="uk-input" type="password" placeholder="Password">
                                                </div>
                                            </div>

                                            <div class="uk-margin">
                                                <a href="#">¿Olvidaste tu password?</a>
                                            </div>

                                            <div class="uk-margin">
                                                <button type="submit" class="uk-button uk-button-primary">
                                                    <span class="ion-forward"></span>&nbsp; Login
                                                </button>
                                            </div>

                                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">

                                            <hr />

                                            <center>
                                                <p>
                                                    ¿No tienes cuenta?
                                                </p>
                                                <a href="/signup" class="uk-button uk-button-default">
                                                    <span class="ion-android-person-add"></span>&nbsp; Registrar
                                                </a>
                                            </center>
                                        </fieldset>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <div class="uk-width-1-1@s uk-width-1-2@l uk-width-1-1@xl"></div>
                    </div>
                </div>
            </div>
        </div>
</div>
